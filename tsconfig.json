{
  "compilerOptions": {
    "noEmit": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "sourceMap": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": false
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}
